<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="280.0" prefWidth="223.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/16" fx:controller="com.example.vavaregexchecker.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <TextField fx:id="textF" />
   <TextArea fx:id="textA" prefHeight="200.0" prefWidth="200.0" />

    <Label fx:id="welcomeText" />
   <Label fx:id="result" prefHeight="17.0" prefWidth="40.0" />
    <Button onAction="#onHelloButtonClick" text="Check" />
</VBox>
